pipeline {
  agent any
  tools {
    maven 'M2_HOME'
        }
stages {
  stage('git checkout') {
  steps {
   git 'https://github.com/jahnavimandarapu/test.git'
  }
}
stage('build package'){
    steps {
      sh 'mvn package'
    }
  }
stage('building docker image'){
    steps {
      sh 'docker build -t mandarapujahnavi/springbootnew:2.0 .'
    }
            }
}
        }
